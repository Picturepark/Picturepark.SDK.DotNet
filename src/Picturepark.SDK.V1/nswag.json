{
  "swaggerGenerator": {
    "fromSwagger": {
      "url": "../../swagger/PictureparkSwagger.json",
      "output": null
    }
  },
  "codeGenerators": {
    "swaggerToCSharpClient": {
      "clientBaseClass": "ClientBase",
      "configurationClass": "Picturepark.SDK.V1.Contract.IPictureparkClientSettings",
      "generateClientClasses": true,
      "generateClientInterfaces": true,
      "generateDtoTypes": true,
      "injectHttpClient": true,
      "protectedMethods": [
        "BusinessProcessClient.WaitForCompletionAsync",
        "BusinessProcessClient.WaitAsync",
        "ListItemClient.CreateManyAsync",
        "SchemaClient.UpdateAsync",
        "SchemaClient.DeleteAsync",
        "JsonSchemaClient.GetAsync"
      ],
      "generateExceptionClasses": true,
      "exceptionClass": "ApiException",
      "wrapDtoExceptions": false,
      "useHttpClientCreationMethod": false,
      "httpClientType": "System.Net.Http.HttpClient",
      "useHttpRequestMessageCreationMethod": true,
      "useBaseUrl": true,
      "generateSyncMethods": true,
      "clientClassAccessModifier": "public",
      "generateContractsOutput": true,
      "contractsNamespace": "Picturepark.SDK.V1.Contract",
      "contractsOutputFilePath": "../Picturepark.SDK.V1.Contract/Contracts.Generated.cs",
      "className": "{controller}Client",
      "namespace": "Picturepark.SDK.V1",
      "additionalNamespaceUsages": [],
      "generateOptionalParameters": true,
      "requiredPropertiesMustBeDefined": true,
      "dateType": "System.DateTime",
      "dateTimeType": "System.DateTime",
      "timeType": "System.TimeSpan",
      "timeSpanType": "System.TimeSpan",
      "arrayType": "System.Collections.Generic.List",
      "dictionaryType": "System.Collections.Generic.Dictionary",
      "classStyle": "Poco",
      "operationGenerationMode": "MultipleClientsFromOperationId",
      "generateDefaultValues": true,
      "generateDataAnnotations": true,
      "excludedTypeNames": [],
      "wrapResponses": false,
      "generateResponseClasses": true,
      "responseClass": "SwaggerResponse",
      "handleReferences": false,
      "generateImmutableArrayProperties": false,
      "generateImmutableDictionaryProperties": false,
      "output": "Generated/Clients.Generated.cs"
    }
  }
}