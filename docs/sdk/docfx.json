{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "Picturepark.SDK.V1/**/*.cs",
            "Picturepark.SDK.V1.Contract/**/*.cs",
            "Picturepark.SDK.V1.Localization/**/*.cs",
            "Picturepark.SDK.V1.ServiceProvider/**/*.cs",
            "Picturepark.SDK.V1.CloudManager/**/*.cs"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**"
          ],
          "cwd": "../../src"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "template": [
      "default",
      "docfx-tmpl"
    ],
    "content": [
      {
        "files": [
          "**/*.yml"
        ],
        "cwd": "obj/api",
        "dest": "api"
      },
      {
        "files": [
          "index.md",
          "toc.yml"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Picturepark.SDK.V1 API",
      "_enableSearch": true
    },
    "dest": "site"
  }
}